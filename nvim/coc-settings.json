{
  "coc.preferences.bracketEnterImprove": true,
  "coc.preferences.": true,
  "prettier.single_quote": true,
  "prettier.trailing_comma": "all",
  "http.proxyStrictSSL": false,
  "eslint.autoFixOnSave": false,
  "eslint.packageManager": "yarn",
  "tsserver.maxTsServerMemory": 8192,
  "tsserver.disableAutomaticTypeAcquisition": false,
  "tsserver.implicitProjectConfig.experimentalDecorators": true,
  "tsserver.log": "verbose",
  "go.goplsOptions": {
    "usePlaceholders": true,
    "completionDocumentation": true,
    "completeUnimported": true,
    "matcher": "Fuzzy"
  },
  "jest.watchman": true,
  "languageserver": {
    "godot": {
      "host": "127.0.0.1",
      "filetypes": ["gd", "gdscript3"],
      "port": 6008
    },
    "haskell": {
      "command": "haskell-language-server-wrapper",
      "args": ["--lsp"],
      "rootPatterns": [
        "*.cabal",
        "stack.yaml",
        "cabal.project",
        "package.yaml",
        "hie.yaml"
      ],
      "filetypes": ["haskell", "lhaskell"]
    }
  },
  "Lua.diagnostics.globals": ["vim"]
}
